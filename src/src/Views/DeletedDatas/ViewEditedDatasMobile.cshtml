@model src.Models.Organization
@inject UserManager<ApplicationUser> UserManager
@{
    ViewData["Title"] = "Audit Trail";
    ApplicationUser appUser = await UserManager.GetUserAsync(User);
    Layout = "_LayoutAdminlteDeletedMobile";
}
    <!-- Content Header (Page header) -->
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-lg-12">
                <div class="box box-primary">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-lg-12">
                                <input type="hidden" asp-for="@Model.organizationId" />
                                <table id="grid" name="grid" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th class="col-md-1">Datetime edited</th>
                                            <th class="col-md-1">Control number</th>
                                            <th class="col-md-1">From</th>
                                            <th class="col-md-1">Original datas</th>
                                            <th class="col-md-1">Edited by</th>
                                            <th class="col-md-1">Remarks</th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>

    </section>

