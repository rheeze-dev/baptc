@model src.Models.PackersAndPorters
@{
    Layout = null;
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title">View Data</h4>
</div>

<form onsubmit="return SubmitAddEdit(this)">
    <div class="box-body">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="Id" />

        <div class="row">
            <div class="col-lg-6 form-group">
                <label asp-for="DateOfApplication">Date: </label>
                <label class="text-warning">@Model.DateOfApplication.ToString("MMMM dd, yyyy")</label>
            </div>
            <div class="col-lg-6 form-group">
                <label asp-for="NameOfAssociation">Association: </label>
                <label class="text-warning">@Model.NameOfAssociation</label>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 form-group">
                <label asp-for="PackerOrPorter">Packer or porter: </label>
                <label class="text-warning">@Model.PackerOrPorter</label>
            </div>
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="IdNumber">Id number: </label>
                    <label class="text-warning">@Model.IdNumber</label>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="Name">Name: </label>
                    <label class="text-warning">@Model.Name</label>
                </div>
            </div>
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="NickName">NickName: </label>
                    <label class="text-warning">@Model.NickName</label>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="NameOfSpouse">Name of spouse: </label>
                    <label class="text-warning">@Model.NameOfSpouse</label>
                </div>
            </div>
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="PresentAddress">Present address: </label>
                    <label class="text-warning">@Model.PresentAddress</label>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="Barangay">Barangay: </label>
                    <label class="text-warning">@Model.Barangay</label>
                </div>
            </div>
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="Municipality">Municipality: </label>
                    <label class="text-warning">@Model.Municipality</label>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="Province">Province: </label>
                    <label class="text-warning">@Model.Province</label>
                </div>
            </div>
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="ContactNumber">Contact number: </label>
                    <label class="text-warning">@Model.ContactNumber</label>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="BirthDate">Birthdate: </label>
                    <label class="text-warning">@Model.BirthDate</label>
                </div>
            </div>
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="ProvincialAddress">Provincial address: </label>
                    <label class="text-warning">@Model.ProvincialAddress</label>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="Requirements">Requirements: </label>
                    <label class="text-warning">@Model.Requirements</label>
                </div>
            </div>
            <div class="col-lg-6 form-group">
                <div class="form-group">
                    <label asp-for="Id">Control number: </label>
                    <label class="text-warning">@Model.Id</label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="form-group">
                <label asp-for="Remarks">Remarks: </label>
                <label class="text-warning">@Model.Remarks</label>
            </div>
        </div>

    </div>
    <div class="box-footer">
        <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
    </div>
</form>



